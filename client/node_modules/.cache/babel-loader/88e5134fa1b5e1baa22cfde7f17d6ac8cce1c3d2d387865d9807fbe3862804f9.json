{"ast":null,"code":"import { defineComponent } from 'vue';\nimport Chart from 'chart.js/auto';\nexport default defineComponent({\n  name: 'StudentDetailsModal',\n  props: {\n    show: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    yearLevel: {\n      type: String,\n      default: ''\n    },\n    section: {\n      type: String,\n      default: ''\n    },\n    subject: {\n      type: String,\n      default: ''\n    },\n    title: {\n      type: String,\n      default: 'Student Details'\n    },\n    chartTitle: {\n      type: String,\n      default: 'Performance Overview'\n    },\n    historyTitle: {\n      type: String,\n      default: 'History'\n    },\n    tableHeaders: {\n      type: Array,\n      default: () => ['Date', 'Status']\n    },\n    chartId: {\n      type: String,\n      default: 'studentChart'\n    },\n    isClassRecord: {\n      type: Boolean,\n      default: false\n    },\n    chartData: {\n      type: Object,\n      required: true,\n      default: () => ({\n        labels: [],\n        datasets: []\n      })\n    }\n  },\n  emits: ['close', 'update:show', 'date-filter-change'],\n  data() {\n    const today = new Date().toISOString().split('T')[0];\n    const thirtyDaysAgo = new Date();\n    thirtyDaysAgo.setDate(thirtyDaysAgo.getDate() - 30);\n    const thirtyDaysAgoStr = thirtyDaysAgo.toISOString().split('T')[0];\n    return {\n      dateFilter: {\n        start: thirtyDaysAgoStr,\n        end: today\n      },\n      chart: null,\n      chartInstance: null\n    };\n  },\n  watch: {\n    chartData: {\n      handler(newData) {\n        this.updateChart(newData);\n      },\n      deep: true,\n      immediate: true\n    },\n    show: {\n      handler(newVal) {\n        if (newVal) {\n          this.$nextTick(() => {\n            this.initChart();\n          });\n        }\n      },\n      immediate: true\n    },\n    dateFilter: {\n      handler(newFilter) {\n        this.$emit('date-filter-change', newFilter);\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  mounted() {\n    this.initChart();\n    // Initial date filter emit\n    this.$emit('date-filter-change', this.dateFilter);\n  },\n  beforeUnmount() {\n    if (this.chartInstance) {\n      this.chartInstance.destroy();\n    }\n  },\n  methods: {\n    closeModal() {\n      this.$emit('update:show', false);\n      this.$emit('close');\n    },\n    handleDateFilterChange() {\n      this.$emit('date-filter-change', this.dateFilter);\n    },\n    initChart() {\n      if (!this.$refs.chartCanvas) return;\n      if (this.chartInstance) {\n        this.chartInstance.destroy();\n      }\n      const ctx = this.$refs.chartCanvas.getContext('2d');\n      this.chartInstance = new Chart(ctx, {\n        type: 'line',\n        data: this.chartData,\n        options: {\n          responsive: true,\n          maintainAspectRatio: false,\n          scales: {\n            y: {\n              beginAtZero: true,\n              title: {\n                display: true,\n                text: 'Score'\n              }\n            },\n            x: {\n              title: {\n                display: true,\n                text: 'Date'\n              }\n            }\n          },\n          plugins: {\n            legend: {\n              display: true,\n              position: 'top'\n            }\n          }\n        }\n      });\n    },\n    updateChart(newData) {\n      if (this.chartInstance) {\n        this.chartInstance.data = newData;\n        this.chartInstance.update('active');\n      }\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","Chart","name","props","show","type","Boolean","required","student","Object","yearLevel","String","default","section","subject","title","chartTitle","historyTitle","tableHeaders","Array","chartId","isClassRecord","chartData","labels","datasets","emits","data","today","Date","toISOString","split","thirtyDaysAgo","setDate","getDate","thirtyDaysAgoStr","dateFilter","start","end","chart","chartInstance","watch","handler","newData","updateChart","deep","immediate","newVal","$nextTick","initChart","newFilter","$emit","mounted","beforeUnmount","destroy","methods","closeModal","handleDateFilterChange","$refs","chartCanvas","ctx","getContext","options","responsive","maintainAspectRatio","scales","y","beginAtZero","display","text","x","plugins","legend","position","update"],"sources":["D:\\au_dev\\client\\src\\components\\modals\\StudentDetailsModal.vue"],"sourcesContent":["<template>\r\n  <teleport to=\"body\">\r\n    <div v-if=\"show\" class=\"modal-overlay\" @click.self=\"closeModal\">\r\n      <div class=\"modal-wrapper\" @click.stop>\r\n        <div class=\"modal-dialog modal-xxl\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header bg-primary text-white\">\r\n              <h5 class=\"modal-title\">\r\n                <i class=\"fas fa-user-graduate me-2\"></i>\r\n                {{ title }}\r\n              </h5>\r\n              <button type=\"button\" class=\"btn-close btn-close-white\" @click=\"closeModal\"></button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"student-details\">\r\n                <!-- Student Information (Left Side) -->\r\n                <div class=\"student-info\">\r\n                  <h6 class=\"info-title\">Student Information</h6>\r\n                  <div class=\"info-content\">\r\n                    <p><strong>Student ID:</strong> {{ student?.studentNumber }}</p>\r\n                    <p><strong>Name:</strong> {{ student?.firstName }} {{ student?.lastName }}</p>\r\n                    <p><strong>Year Level:</strong> {{ yearLevel }}</p>\r\n                    <p><strong>Section:</strong> {{ section }}</p>\r\n                    <p><strong>Subject:</strong> {{ subject }}</p>\r\n                    <slot name=\"additional-info\"></slot>\r\n                  </div>\r\n                </div>\r\n\r\n                <!-- Performance and Data (Right Side) -->\r\n                <div class=\"student-charts\">\r\n                  <!-- Multiple Chart Sections for Class Records -->\r\n                  <div v-if=\"isClassRecord\" class=\"charts-container\">\r\n                    <!-- Date Filter -->\r\n                    <div class=\"chart-controls mb-3\">\r\n                      <div class=\"date-filter\">\r\n                        <div class=\"input-group input-group-sm\">\r\n                          <span class=\"input-group-text\">From</span>\r\n                          <input \r\n                            type=\"date\" \r\n                            class=\"form-control\" \r\n                            v-model=\"dateFilter.start\"\r\n                            @change=\"handleDateFilterChange\"\r\n                            :max=\"dateFilter.end\"\r\n                          >\r\n                          <span class=\"input-group-text\">To</span>\r\n                          <input \r\n                            type=\"date\" \r\n                            class=\"form-control\" \r\n                            v-model=\"dateFilter.end\"\r\n                            @change=\"handleDateFilterChange\"\r\n                            :min=\"dateFilter.start\"\r\n                          >\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    \r\n                    <!-- Single Chart for All Assessments -->\r\n                    <div class=\"chart-section\">\r\n                      <h6>All Assessments</h6>\r\n                      <div class=\"chart-container\" style=\"height: 300px;\">\r\n                        <canvas ref=\"chartCanvas\"></canvas>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                  <!-- Single Chart Section for Attendance -->\r\n                  <div v-else class=\"char