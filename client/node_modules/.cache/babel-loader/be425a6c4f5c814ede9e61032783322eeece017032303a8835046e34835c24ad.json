">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header bg-primary text-white\">\r\n              <h5 class=\"modal-title\">\r\n                <i class=\"fas fa-user-graduate me-2\"></i>\r\n                {{ title }}\r\n              </h5>\r\n              <button type=\"button\" class=\"btn-close btn-close-white\" @click=\"closeModal\"></button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"student-details\">\r\n                <!-- Student Information (Left Side) -->\r\n                <div class=\"student-info\">\r\n                  <h6 class=\"info-title\">Student Information</h6>\r\n                  <div class=\"info-content\">\r\n                    <p><strong>Student ID:</strong> {{ student?.studentNumber }}</p>\r\n                    <p><strong>Name:</strong> {{ student?.firstName }} {{ student?.lastName }}</p>\r\n                    <p><strong>Year Level:</strong> {{ yearLevel }}</p>\r\n                    <p><strong>Section:</strong> {{ section }}</p>\r\n                    <p><strong>Subject:</strong> {{ subject }}</p>\r\n                    <slot name=\"additional-info\"></slot>\r\n                  </div>\r\n                </div>\r\n\r\n                <!-- Performance and Data (Right Side) -->\r\n                <div class=\"student-charts\">\r\n                  <!-- Multiple Chart Sections for Class Records -->\r\n                  <div v-if=\"isClassRecord\" class=\"charts-container\">\r\n                    <!-- Date Filter -->\r\n                    <div class=\"chart-controls mb-3\">\r\n                      <div class=\"date-filter\">\r\n                        <div class=\"input-group input-group-sm\">\r\n                          <span class=\"input-group-text\">From</span>\r\n                          <input \r\n                            type=\"date\" \r\n                            class=\"form-control\" \r\n                            v-model=\"dateFilter.start\"\r\n                            @change=\"handleDateFilterChange\"\r\n                            :max=\"dateFilter.end\"\r\n                          >\r\n                          <span class=\"input-group-text\">To</span>\r\n                          <input \r\n                            type=\"date\" \r\n                            class=\"form-control\" \r\n                            v-model=\"dateFilter.end\"\r\n                            @change=\"handleDateFilterChange\"\r\n                            :min=\"dateFilter.start\"\r\n                          >\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    \r\n                    <!-- Single Chart for All Assessments -->\r\n                    <div class=\"chart-section\">\r\n                      <h6>All Assessments</h6>\r\n                      <div class=\"chart-container\" style=\"height: 300px;\">\r\n                        <canvas ref=\"chartCanvas\"></canvas>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                  <!-- Single Chart Section for Attendance -->\r\n                  <div v-else class=\"chart-section\">\r\n                    <h6>{{ chartTitle }}</h6>\r\n                    <div class=\"chart-container\" style=\"height: 300px;\">\r\n                      <canvas :id=\"chartId\"></canvas>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                  <!-- History Section -->\r\n                  <div class=\"chart-section\">\r\n                    <h6>{{ historyTitle }}</h6>\r\n                    <div class=\"table-responsive data-table\">\r\n                      <slot name=\"history-table\">\r\n                        <!-- Default history table if no slot provided -->\r\n                        <table class=\"table\">\r\n                          <thead>\r\n                            <tr>\r\n                              <th v-for=\"header in tableHeaders\" :key=\"header\">{{ header }}</th>\r\n                            </tr>\r\n                          </thead>\r\n                          <tbody>\r\n                            <slot name=\"table-rows\">\r\n                              <tr>\r\n                                <td colspan=\"100%\" class=\"text-center py-3 text-muted\">\r\n                                  <i class=\"fas fa-info-circle me-2\"></i>No records found.\r\n                                </td>\r\n                              </tr>\r\n                            </slot>\r\n                          </tbody>\r\n                        </table>\r\n                      </slot>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"modal-footer\">\r\n              <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                <i class=\"fas fa-times me-2\"></i>Close\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"modal-backdrop\" @click=\"closeModal\"></div>\r\n    </div>\r\n  </teleport>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent } from 'vue'\r\nimport Chart from 'chart.js/auto'\r\n\r\nexport default defineComponent({\r\n  name: 'StudentDetailsModal',\r\n  \r\n  props: {\r\n    show: {\r\n      type: Boolean,\r\n      required: true\r\n    },\r\n    student: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    yearLevel: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    section: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    subject: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    title: {\r\n      type: String,\r\n      default: 'Student Details'\r\n    },\r\n    chartTitle: {\r\n      type: String,\r\n      default: 'Performance Overview'\r\n    },\r\n    historyTitle: {\r\n      type: String,\r\n      default: 'History'\r\n    },\r\n    tableHeaders: {\r\n      type: Array,\r\n      default: () => ['Date', 'Status']\r\n    },\r\n    chartId: {\r\n      type: String,\r\n      default: 'studentChart'\r\n    },\r\n    isClassRecord: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    chartData: {\r\n      type: Object,\r\n      required: true,\r\n      default: () => ({\r\n        labels: [],\r\n        datasets: []\r\n      })\r\n    }\r\n  },\r\n\r\n  emits: ['close', 'update:show', 'date-filter-change'],\r\n\r\n  data() {\r\n    const today = new Date().toISOString().split('T')[0];\r\n    const thirtyDaysAgo = new Date();\r\n    thirtyDaysAgo.setDate(thirtyDaysAgo.getDate() - 30);\r\n    const thirtyDaysAgoStr = thirtyDaysAgo.toISOString().split('T')[0];\r\n    \r\n    return {\r\n      dateFilter: {\r\n        start: thirtyDaysAgoStr,\r\n        end: today\r\n      },\r\n      chart: null,\r\n      chartInstance: null\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    chartData: {\r\n      handler(newData) {\r\n        this.updateChart(newData);\r\n      },\r\n      deep: true,\r\n      immediate: true\r\n    },\r\n    \r\n    show: {\r\n      handler(newVal) {\r\n        if (newVal) {\r\n          this.$nextTick(() => {\r\n            this.initChart();\r\n          });\r\n        }\r\n      },\r\n      immediate: true\r\n    },\r\n\r\n    dateFilter: {\r\n      handler(newFilter) {\r\n        this.$emit('date-filter-change', newFilter);\r\n      },\r\n      deep: true,\r\n      immediate: true\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.initChart();\r\n    // Initial date filter emit\r\n    this.$emit('date-filter-change', this.dateFilter);\r\n  },\r\n\r\n  beforeUnmount() {\r\n    if (this.chartInstance) {\r\n      this.chartInstance.destroy();\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    closeModal() {\r\n      this.$emit('update:show', false)\r\n      this.$emit('close')\r\n    },\r\n\r\n    ha